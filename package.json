{
    "name": "pc-nrfconnect-tracecollector-preview",
    "version": "0.3.7",
    "description": "EXPERIMENTAL: Capture and convert nRF91 modem traces",
    "displayName": "Trace Collector V2 preview",
    "homepage": "https://github.com/NordicPlayground/pc-nrfconnect-cellularmonitor",
    "repository": {
        "type": "git",
        "url": "https://github.com/NordicPlayground/pc-nrfconnect-cellularmonitor.git"
    },
    "author": "Nordic Semiconductor ASA",
    "license": "SEE LICENSE IN LICENSE",
    "engines": {
        "nrfconnect": ">=4.0.0"
    },
    "main": "dist/bundle.js",
    "files": [
        "dist/",
        "resources/*",
        "LICENSE"
    ],
    "scripts": {
        "watch": "run-p --silent --continue-on-error watch:*",
        "watch:build": "run-esbuild --watch",
        "watch:types": "tsc --noEmit --pretty --watch --preserveWatchOutput",
        "build:dev": "run-esbuild",
        "build:prod": "run-esbuild --prod",
        "test": "jest",
        "check": "run-p --silent --continue-on-error --print-label \"check:*\"",
        "check:app": "check-app-properties",
        "check:lint": "eslint --color .",
        "check:types": "check-for-typescript tsc --noEmit --pretty",
        "check:license": "nrfconnect-license check",
        "nordic-publish": "node ./dist/nordic-publish.js",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@types/node": "15.14.1",
        "@types/plotly.js": "^1.54.19",
        "@types/redux-mock-store": "^1.0.2",
        "ansi-colors": "^4.1.1",
        "ansi-escapes": "^4.3.1",
        "check-disk-space": "^2.1.0",
        "dangerously-set-html-content": "^1.0.9",
        "ify-loader": "^1.1.0",
        "jest-fetch-mock": "^3.0.3",
        "pc-nrfconnect-shared": "git+https://github.com/NordicSemiconductor/pc-nrfconnect-shared.git#v6.14.3",
        "pc-xterm-lib": "git+https://github.com/NordicPlayground/pc-xterm-lib#6934b5b769b942945527a79787732bec6874309f",
        "plotly.js": "^2.8.3",
        "pretty-bytes": "^5.6.0",
        "redux-mock-store": "^1.5.4",
        "xterm-addon-fit": "^0.5.0",
        "xterm-for-react": "^1.0.4"
    },
    "dependencies": {
        "@nordicsemiconductor/nrf-monitor-lib-js": "^0.7.6",
        "npm": "^6.14.13"
    },
    "bundledDependencies": [
        "@nordicsemiconductor/nrf-monitor-lib-js"
    ],
    "eslintConfig": {
        "extends": "./node_modules/pc-nrfconnect-shared/config/eslintrc"
    },
    "prettier": "./node_modules/pc-nrfconnect-shared/config/prettier.config.js"
}
